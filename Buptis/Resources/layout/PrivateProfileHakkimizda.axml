<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
	android:background="#221e20"
	android:id="@+id/rootView"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
		<LinearLayout
		android:background="@drawable/lokasyonlarheadergradient"
		android:orientation="vertical"
		android:minWidth="25px"
		android:minHeight="25px"
		android:layout_width="match_parent"
		android:layout_height="105.5dp"
		android:id="@+id/linearLayout1">
		<RelativeLayout
			android:layout_marginTop="39.5dp"
			android:layout_marginLeft="15dp"
			android:layout_marginRight="20dp"
			android:minWidth="25px"
			android:minHeight="25px"
			android:layout_width="match_parent"
			android:layout_height="40dp"
			android:id="@+id/relativeLayout1">
			<ImageButton
				android:padding="10dp"
				android:background="@null"
				android:scaleType="centerInside"
				android:src="@mipmap/geriicon"
				android:layout_width="40dp"
				android:layout_height="match_parent"
				android:id="@+id/ımageButton1" />
			<TextView
				android:text="Hakkımızda"
				android:layout_width="150dp"
				android:textColor="#fff"
				android:layout_marginTop="3dp"
				android:layout_marginLeft="5dp"
				android:textSize="22dp"
				android:textStyle="bold"
				android:layout_height="wrap_content"
				android:layout_toRightOf="@+id/ımageButton1"
				android:id="@+id/textView1" />
		</RelativeLayout>
	</LinearLayout>
	
		<LinearLayout
			android:orientation="vertical"
			android:minWidth="25px"
			android:minHeight="25px"
			android:layout_width="match_parent"
			android:layout_height="match_parent"
			android:id="@+id/linearLayout4" >
			<TextView
				android:textSize="15dp"
				android:textStyle="bold"
				android:layout_marginLeft="20dp"
                android:layout_marginTop="20dp"
				android:textColor="#EEEFEF"
				android:text="Şartlar ve Koşullar"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:id="@+id/textView2" />
			<View
				android:layout_margin="20dp"
				android:background="#6E6E6E"
				android:layout_width="match_parent"
				android:layout_height="2dp"
				android:id="@+id/view1" />
			<TextView
				android:textSize="15dp"
				android:textStyle="bold"
				android:layout_marginLeft="20dp"
				android:textColor="#EEEFEF"
				android:text="Gizlilik"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:id="@+id/textView3" />
				<View
				android:layout_margin="20dp"
				android:background="#6E6E6E"
				android:layout_width="match_parent"
				android:layout_height="2dp"
				android:id="@+id/view2" />
        	<TextView
				android:textSize="15dp"
				android:textStyle="bold"
				android:layout_marginLeft="20dp"
				android:textColor="#EEEFEF"
				android:text="Buptis Hakkında"
				android:textAppearance="?android:attr/textAppearanceMedium"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:id="@+id/textView5" />
            <TextView
				android:textSize="15dp"
				android:layout_marginLeft="20dp"
				android:textColor="#EEEFEF"
				android:text="v1.112.1"
				android:textAppearance="?android:attr/textAppearanceSmall"
				android:layout_width="match_parent"
				android:layout_height="wrap_content"
				android:id="@+id/veriyontxt" />
				<View
				android:layout_margin="20dp"
				android:background="#6E6E6E"
				android:layout_width="match_parent"
				android:layout_height="2dp"
				android:id="@+id/view3" />
		</LinearLayout>
	
</LinearLayout>
